{
    "name": "alt-three/bus",
    "description": "An improved command bus for Laravel 5.2+",
    "keywords": ["bus", "command", "job", "command bus", "Bus", "Alt Three"],
    "license": "MIT",
    "authors": [
        {
            "name": "James Brooks",
            "email": "james@alt-three.com"
        },
        {
            "name": "Graham Campbell",
            "email": "graham@alt-three.com"
        },
        {
            "name": "Joseph Cohen",
            "email": "joe@alt-three.com"
        },
        {
            "name": "Taylor Otwell",
            "email": "taylorotwell@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "illuminate/bus": "5.3.*|5.4.*",
        "illuminate/container": "5.3.*|5.4.*",
        "illuminate/contracts": "5.3.*|5.4.*",
        "illuminate/pipeline": "5.3.*|5.4.*",
        "illuminate/support": "5.3.*|5.4.*"
    },
    "require-dev": {
        "graham-campbell/testbench": "^4.0",
        "mockery/mockery": "^0.9.9",
        "phpunit/phpunit": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "AltThree\\Bus\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
