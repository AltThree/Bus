{
    "name": "alt-three/bus",
    "description": "An improved command bus for Laravel 5",
    "keywords": ["bus", "command", "job", "command bus", "Bus", "Alt Three"],
    "license": "MIT",
    "authors": [
        {
            "name": "Alt Three",
            "email": "support@alt-three.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/bus": "^5.5|^6.0",
        "illuminate/container": "^5.5|^6.0",
        "illuminate/contracts": "^5.5|^6.0",
        "illuminate/pipeline": "^5.5|^6.0",
        "illuminate/support": "^5.5|^6.0"
    },
    "require-dev": {
        "graham-campbell/analyzer": "^2.1",
        "graham-campbell/testbench": "^5.2",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.5|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "AltThree\\Bus\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.3-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
